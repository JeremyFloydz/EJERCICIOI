<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<FlowPane xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.ejei.EditarPersonaController">
    <children>
        <HBox prefHeight="32.0" prefWidth="506.0">
            <children>
                <Label text="Nombre" wrapText="true">
                    <Font size="14.0" />
                    <HBox.margin>
                        <Insets left="10.0" top="7.0" />
                    </HBox.margin>
                </Label>
                <TextField fx:id="nombreField" prefHeight="26.0" prefWidth="347.0">
               <HBox.margin>
                  <Insets left="17.0" />
               </HBox.margin></TextField>
            </children>
            <FlowPane.margin>
                <Insets top="20.0" />
            </FlowPane.margin>
        </HBox>
        <HBox prefHeight="32.0" prefWidth="506.0">
            <children>
                <Label text="Apellidos" wrapText="true">
                    <Font size="14.0" />
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0" />
                    </HBox.margin>
                </Label>
                <TextField fx:id="apellidosField" prefHeight="26.0" prefWidth="351.0">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin></TextField>
            </children>
        </HBox>
        <HBox prefHeight="32.0" prefWidth="506.0">
            <children>
                <Label text="Edad" wrapText="true">
                    <Font size="14.0" />
                    <HBox.margin>
                        <Insets left="25.0" top="7.0" />
                    </HBox.margin>
                </Label>
                <TextField fx:id="edadField" prefHeight="26.0" prefWidth="114.0">
               <HBox.margin>
                  <Insets left="22.0" />
               </HBox.margin></TextField>
            </children>
        </HBox>
        <HBox prefHeight="62.0" prefWidth="504.0">
            <children>
                <Button fx:id="guardarButton" mnemonicParsing="false" text="Guardar">
               <HBox.margin>
                  <Insets left="140.0" top="15.0" />
               </HBox.margin></Button>
                <Button fx:id="cancelarButton" mnemonicParsing="false" text="Cancelar">
               <HBox.margin>
                  <Insets left="40.0" top="15.0" />
               </HBox.margin></Button>
            </children>
        </HBox>
    </children>
</FlowPane>
